{
    "author": "In6id",
    "action": {
        "default_popup": "Template/auth.html",
        "default_title": "Amexline",
        "matches": [ "*://*.copart.com/*" ]
    },
    "background": {
        "service_worker": "Js/background.js",
        "matches": [ "*://*.copart.com/*" ],
        "type": "module"
    },
    "description": "Amexline Company",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "manifest_version": 3,
    "name": "Amexline",
    "permissions": [ "storage", "cookies", "tabs", "webRequest", "activeTab", "scripting" ],
    "version": "1.0.0",
    "host_permissions": [ "*://*.copart.com/*" ],
    "content_scripts": [ {
        "matches": [ "*://*.copart.com/*" ],
        "js": [ "Js/background.js" ],
        "run_at": "document_end"
    },
    {
        "matches": [ "*://*.copart.com/*" ],
        "js": [ "Js/htmlParse.js" ]
    },
    {
        "matches": [ "*://*.copart.com/*" ],
        "js": [ "Js/content.js" ],
        "run_at": "document_start"
    }
 ]
}